
<%- include("partials/header"); -%>

<div class="container-fluid">
    <div class="row">
        <div class="col-md-6 mx-auto d-flex flex-column">
            <h3 class="p-2">Account Setting</h3>
            <label class="p-2" for="username">Username: </label>
            <input class="p-2" type="text" id="username" value="<%= username %>">
            <label class="p-2" for="email">Email: </label>
            <input type="email" id="email" value="<%= email%>">
            <label for="password">Password: </label>
            <input type="password" id="password" value="">
        </div>
    </div>
</div>

<%- include("partials/footer"); -%>
